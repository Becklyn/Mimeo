{
    "name": "becklyn/mimeo",
    "type": "symfony-bundle",
    "description": "Mimeo: copy (static) assets from NPM packages into your symfony project.",
    "keywords": [
        "becklyn",
        "symfony",
        "npm",
        "package.json",
        "mirror"
    ],
    "homepage": "https://github.com/Becklyn/Mimeo",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/config": "^4.2 || ^5.0",
        "symfony/console": "^4.2 || ^5.0",
        "symfony/dependency-injection": "^4.2 || ^5.0",
        "symfony/http-kernel": "^4.2 || ^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Mimeo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\Mimeo\\": "tests/"
        }
    }
}
